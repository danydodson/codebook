{
  "name": "codebook",
  "version": "1.0.0",
  "description": "JavaScript Code Book",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8080",
    "develop:pg": "yarn run clean && GATSBY_GRAPHQL_IDE=playground gatsby develop -H 0.0.0.0 -p 8080",
    "start": "yarn run develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "heroku-postbuild": "gatsby build"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "3.5.1",
    "gatsby-plugin-manifest": "3.5.0",
    "gatsby-plugin-react-helmet": "4.5.0",
    "gatsby-plugin-sharp": "3.5.0",
    "gatsby-plugin-styled-components": "^4.5.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "3.5.0",
    "gatsby-transformer-remark": "4.2.0",
    "gatsby-transformer-sharp": "3.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "prettier": "2.3.0"
  },
  "author": "Dany Dodson <danydodson@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "javascript",
    "mdx"
  ]
}
